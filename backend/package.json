{
  "name": "backends",
  "version": "0.0.1",
  "description": "my cat&#39;s pajamas project",
  "repository": {
    "url": "git+https://github.com/gengjiawen/backends.git",
    "type": "git"
  },
  "main": "build/index.js",
  "license": "MIT",
  "author": "gengjiawen <jiawen.geng@vivo.com>",
  "scripts": {
    "start": "tsc -w",
    "dev": "nodemon --exec ts-node libs/index.ts",
    "clean": "rimraf build",
    "format": "prettier --write \"{examples,libs,script,bin}/**/*.{js,ts}\" \"**/*.yml\"",
    "format:check": "prettier --list-different \"{examples,libs,script,bin}/**/*.{js,ts}\" \"**/*.yml\"",
    "test": "jest",
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "postbuild": "cpy '**/*' '!**/*.ts' ../build/ --cwd=libs --parents"
  },
  "dependencies": {
    "@koa/cors": "4.0.0",
    "@koa/router": "^12.0.0",
    "@types/koa": "^2.13.5",
    "@types/koa__cors": "4.0.0",
    "@types/koa__router": "12.0.0",
    "crypto-random-string": "5.0.0",
    "execa": "5.1.1",
    "koa": "^2.14.1",
    "koa-basic-auth": "^4.0.0",
    "koa-body": "6.0.1",
    "koa-compress": "^5.1.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@types/node": "18.15.5",
    "cpy-cli": "^4.2.0",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.6",
    "rimraf": "4.4.1",
    "ts-jest": "29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "5.0.2"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "bugs": {
    "url": "https://github.com/gengjiawen/backends/issues"
  },
  "homepage": "https://github.com/gengjiawen/backends#readme",
  "keywords": []
}
