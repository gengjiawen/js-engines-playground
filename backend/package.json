{
  "name": "backends",
  "version": "0.0.1",
  "description": "my cat&#39;s pajamas project",
  "repository": {
    "url": "git+https://github.com/gengjiawen/backends.git",
    "type": "git"
  },
  "main": "build/index.js",
  "license": "MIT",
  "author": "gengjiawen <jiawen.geng@vivo.com>",
  "scripts": {
    "start": "tsc -w",
    "dev": "nodemon --exec ts-node libs/index.ts",
    "clean": "rimraf build",
    "format": "prettier --write \"{examples,libs,script,bin}/**/*.{js,ts}\" \"**/*.yml\"",
    "format:check": "prettier --list-different \"{examples,libs,script,bin}/**/*.{js,ts}\" \"**/*.yml\"",
    "test": "jest",
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "postbuild": "cpy '**/*' '!**/*.ts' ../build/ --cwd=libs --parents"
  },
  "dependencies": {
    "@koa/cors": "3.3.0",
    "@koa/router": "^11.0.1",
    "@types/koa": "^2.13.4",
    "@types/koa__cors": "3.3.0",
    "@types/koa__router": "8.0.11",
    "execa": "5.1.1",
    "koa": "^2.13.4",
    "koa-basic-auth": "^4.0.0",
    "koa-body": "5.0.0",
    "koa-compress": "^5.1.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "tempy": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "28.1.4",
    "@types/node": "18.0.3",
    "cpy-cli": "^4.1.0",
    "husky": "^8.0.1",
    "jest": "28.1.2",
    "lint-staged": "^13.0.3",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,md,css,html,yml}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "bugs": {
    "url": "https://github.com/gengjiawen/backends/issues"
  },
  "homepage": "https://github.com/gengjiawen/backends#readme",
  "keywords": []
}
